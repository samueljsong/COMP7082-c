{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "plugins": ["cypress"],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:cypress/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "cypress/no-unnecessary-waiting": "off"
    }
}